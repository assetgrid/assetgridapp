{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["./tsconfig.json"]
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["./src/assets/*"],
    "rules": {
        "indent": ["error", 4],
        "@typescript-eslint/indent": ["error", 4]
    }
}
